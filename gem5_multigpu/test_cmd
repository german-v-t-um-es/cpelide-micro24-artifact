docker run --rm -u $UID:$GID --volume $(pwd):$(pwd) -w $(pwd)/multigpu_benchmarks/square/ pdalmia/cpcoh make square_m
docker run --rm --volume $(pwd):$(pwd) -w $(pwd) pdalmia/cpcoh scons build/GCN3_X86/gem5.opt -j$(nproc)
docker run --rm --volume $(pwd):$(pwd) -w $(pwd) pdalmia/cpcoh build/GCN3_X86/gem5.opt --debug-flag=CPCoh,GlobalScheduler --debug-file=square_baseline_6 configs/example/apu_se.py -n3 --benchmark-root=multigpu_benchmarks/square/bin -c square_m --options="16384 1 3 0 64 256 0" --num--alloc-policy=dynamic --gs-policy=GSP_RR --gs-num-sched-gpu=1